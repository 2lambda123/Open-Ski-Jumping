    <event-templates>
        <individual name="Qual" hill-surface="Snow" or-type="Classification" or-id="World Cup">
            <classifications>
                <non-ranked id="World Cup"/>
            </classifications>

            <rounds>
                <normal out-limit="50"/>
            </rounds>
        </individual>

        <individual name="Ind" hill-surface="Snow" or-id="World Cup" or-type="Classification" qr-type="Event" qr-id="^1">
            <classifications>
                <ranked id="World Cup"/>
                <ranked id="Nations Cup" bib="false"/>
            </classifications>

            <rounds>
                <normal out-limit="30"/>
                <normal/>
            </rounds>
        </individual>

        <individual name="Ind2" hill-surface="Snow" keep-qr-bibs="true" or-id="World Cup" or-type="Classification" qr-type="Event" qr-id="^1">
            <classifications>
                <ranked id="World Cup"/>
                <ranked id="Nations Cup" bib="false"/>
            </classifications>

            <rounds>
                <normal out-limit="30"/>
                <normal/>
            </rounds>
        </individual>
        
        <team name="Team" hill-surface="Snow" or-id="Nations Cup" or-type="Classification">
            <classifications>
                <ranked id="Nations Cup"/>
            </classifications>

            <rounds>
                <normal out-limit="8" use-ord-rank="1111"/>
                <normal use-ord-rank="0000"/>
            </rounds>
        </team>
    </event-templates>

